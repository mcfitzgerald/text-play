rtnerQuestion": false, "asker": {"id": "VXNlckAwOjIzNTc5MzQyOA==", "__typename": "User", "isEmployee": false}, "debugInfo": "{}", "tribeItem": null, "viewer": {"id": "Vmlld2VyQDA6MA==", "__typename": "Viewer", "uid": null}, "network": {"nid": 0, "id": "TmV0d29yazow"}, "sourceUrl": null, "hadDetails": false, "viewerDisplayCredential": null}, "credibilityFacts": [{"id": "QXV0aG9ySW5mb0ZhY3RAMDoyMzcwOTA3MTM6WzI2NjgsIDM4MDU5OTJd", "__typename": "AuthorInfoFact", "viewCount": 3805992, "answerCount": 2668}], "sourceNetwork": null, "sourceQuestion": null, "sourceAnswer": null, "tribeItem": null, "businessCredential": null, "businessAnswer": null, "viewerHasUpvoted": false, "numSocialVoters": 0, "numUpvotes": 247, "socialVoterNames": [], "socialVoterPhotos": [], "isDeleted": false, "viewerCanRestoreAnswer": false, "showAskerSatisfactionSurvey": false, "suggestedEdit": null, "content": "{\"sections\": [{\"spans\": [{\"text\": \"Thank you for this question, Rose Anne. Prince Harry seems to be waging war on just about everything. Get him on social media or on camera and he begins yammering away about \\u2018systemic racism\\u2019 and that host of other, nouveau terms with racism attached. He loses me and quite frankly, does he mumble now? I cannot understand him.\", \"modifiers\": {}}], \"indent\": 0, \"quoted\": false, \"type\": \"plain\", \"is_rtl\": false}, {\"spans\": [{\"text\": \"Harry now includes himself and his wifey in being SJWs, Social (In)justice Warriors. Harry\\u2019s social injustice was showing before Markle, when he was in the army and called one of his fellow soldiers the N-word. Great, Harry.\", \"modifiers\": {}}], \"indent\": 0, \"quoted\": false, \"type\": \"plain\", \"is_rtl\": false}, {\"spans\": [{\"text\": \"I am able to make out that he is angry at social media for being hypocritical and allowing people to go on undisturbed, for disparaging others. I, for one, become incandescent with rage at the thought of Harry and Markle disrespecting Her Majesty. Enough said.\", \"modifiers\": {}}], \"indent\": 0, \"quoted\": false, \"type\": \"plain\", \"is_rtl\": false}]}", "isSensitive": false, "monetizationAccessOption": null, "numViews": 11677, "isNotForReproduction": false, "moderationCollapseType": null, "hasDisclaimer": false, "disclaimer": null, "numRequesters": 1, "requesters": [{"id": "VXNlckAwOjIzNTc5MzQyOA==", "__typename": "User", "uid": 235793428, "isAnon": false, "useTranslatedName": false, "names": [{"__typename": "UserName", "id": "VXNlck5hbWU6MjM1NzkzNDI4OkxBVE4=", "reverseOrder": false, "givenName": "Rose", "familyName": "Anne Campbell", "scriptCode": "LATN"}], "adminNames": [], "profileUrl": "/profile/Rose-Anne-Campbell"}], "isCommentsDisabled": false, "isCollapsedForViewer": false, "isModerationLocked": false, "moderationCollapseTypeUrl": null, "network": {"id": "TmV0d29yazow", "__typename": "Network", "contentLanguageCode": "en"}, "collapseOperation": null, "originalQuestionIfDifferent": null, "viewerShouldPromptTopicBio": false}, "viewer": {"id": "Vmlld2VyQDA6MA==", "__typename": "Viewer", "user": null}, "notifInjectedBundleFromSncid": null}, "broadcastId": "main-w-chan50-8888-react_gjprknkentnyjtfv-DSRZ", "tchannelData": {"minSeq": "0", "channel": "main-w-chan50-8888-react_gjprknkentnyjtfv-DSRZ", "channelHash": "10257443951055126418", "boxName": "chan50-8888", "baseHost": "quora.com", "targetUrl": "", "enableWebsocket": true}, "broadcastData": {"categoryToDepkeys": {"Viewer:isUniversalLoggedIn:Vmlld2VyQDA6MA==": ["LIUS:5d7419611eb0b54bfacc3f5230fa35c6", "LIUB:644dcd18c53e55bc81a4a9f9eff966f8"]}, "depkeyToVersion": {"LIUS:5d7419611eb0b54bfacc3f5230fa35c6": 0, "LIUB:644dcd18c53e55bc81a4a9f9eff966f8": 0}}}